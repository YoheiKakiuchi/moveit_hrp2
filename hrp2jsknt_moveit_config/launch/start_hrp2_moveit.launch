<launch>
  <arg name="REAL" default="true" />

  <include file="$(find jsk_ik_server)/launch/hrp2jsknt-ik-server.launch" />

  <include if    ="$(arg REAL)"
           file="$(find hrp2jsknt_moveit_config)/launch/HRP2JSKNT_demo.launch">
    <arg name="PUBLISH_BASE_TF" value="false" />
  </include>
  <include unless="$(arg REAL)"
           file="$(find hrp2jsknt_moveit_config)/launch/demo.launch">
    <arg name="PUBLISH_BASE_TF" value="false" />
  </include>

  <node if    ="$(arg REAL)"
        pkg="pr2eus_moveit"
        type="publish-hrp2-moveit-base.l"
        name="publish_moveit_base_tf" />

</launch>